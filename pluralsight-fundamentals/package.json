  "scripts": {
    "aot": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "lint": "node_modules/.bin/tslint \"app/**/*.ts\"",
    "lint:f": "node_modules/.bin/tslint \"app/**/*.ts\" --fix",
    "rollup": "node_modules/.bin/rollup -c rollup.config.js",
